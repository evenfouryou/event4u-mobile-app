{
  "expo": {
    "name": "Event4U",
    "slug": "event4u",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "event4u",
    "userInterfaceStyle": "dark",
    "splash": {
      "backgroundColor": "#0a0e17"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.event4u.app"
    },
    "android": {
      "package": "com.event4u.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
